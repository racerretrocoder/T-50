  <engine>
   <name>ab34</name>
   <mode>looped</mode>
   <path>Aircraft/T-50/Sounds/AB.wav</path>
   <condition>

<greater-than>  
 <property>engines/engine[1]/n1</property>
   <value>90</value>
  </greater-than>

  
     <not>
	 <property>/sim/current-view/internal</property>
<property>/engines/engine/out-of-fuel</property>
     </not>
