   <position>
    	<x>0</x>
	<y>0.0</y>
	<z>0</z>
   </position>
               <orientation>
                <x>1.0</x>
                <y>0.001</y>
                <z>0.001</z>
                <inner-angle>180.0</inner-angle>
                <outer-angle>180.0</outer-angle>
                <outer-gain>0.0</outer-gain>
            </orientation>
      <volume>
    <property>/engines/engine/nozzle-pos-norm</property>
    <factor>1</factor>
    <offset>0</offset>
