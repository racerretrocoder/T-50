    <!-- Sonic Boom -->
  <sonicboom>
   <name>Sonic Boom</name>
    <mode>once</mode>
   <path>Aircraft/T38/Sounds/sonicboom.wav</path>
   <condition>
   <and>
        <greater-than>
            <property>velocities/mach</property>
            <value>1.0</value>
        </greater-than>
        <equals>
            <property>/sim/current-view/internal</property>
            <value>false</value>
        </equals>
    </and>
   </condition>
    <reference-dist>600.0</reference-dist>
    <max-dist>6000.0</max-dist>
  </sonicboom>
 
 
<!-- flyby -->
        <flyby>
            <name>fly-by</name>
            <mode>once</mode>
            <path>Aircraft/T38/Sounds/flyby.wav</path>
            <condition>
                <and>
        <greater-than>
            <property>velocities/mach</property>
            <value>0.8</value>
        </greater-than>
                <equals>
                    <property>sim/current-view/view-number</property>
                    <value>6</value>
                </equals>
                </and>
                <not>
            <property>/sim/current-view/internal</property>
        <property>/engines/engine/out-of-fuel</property>
        </not>
        </condition>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.001</factor>
            </volume>
             
        </flyby>
         
    <crash>
  <name>intense ground contact</name>
  <mode>once</mode>
  <path>Sounds/crash.wav</path>
  <property>sim/multiplay/generic/int[7]</property>
  <volume>
    <factor>1</factor>
  </volume>
  <pitch>
    <factor>1.5</factor>
  </pitch>
</crash>  
     
     
     
    <engine1-wake-forward>
  <name>Left Engine Wake</name>
  <mode>looped</mode>
  <path>Aircraft/T-50/Sounds/wake.wav</path>
  <condition>
   <and>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
    <or>
    <equals>
     <property>/sim/current-view/name</property>
     <value>Fly-By View</value>
    </equals>
    <equals>
     <property>/sim/current-view/name</property>
     <value>Tower View</value>
    </equals>
    <equals>
     <property>/sim/current-view/name</property>
     <value>Tower View Look From</value>
    </equals>
    </or>
   </and>
  </condition>
  <position>
   <x>25</x>
   <y>-0.5</y>
   <z>0</z>
   </position>
   <orientation>
    <x>1</x>
    <y>0</y>
    <z>0</z>
    <inner-angle>15.0</inner-angle>
    <outer-angle>90.0</outer-angle>
    <outer-gain>0.025</outer-gain>
   </orientation>
   <volume>
    <property>velocities/mach</property>
    <factor>1.9</factor>
    <offset>-0.3</offset>
    <min>0.00</min>
    <max>1.0</max>
    </volume>
  </engine1-wake-forward> 
   
  <engine1-wake-forward>
  <name>Left Engine Wake Forward</name>
  <mode>looped</mode>
  <path>Aircraft/T-50/Sounds/wake-fwd.wav</path>
