<PropertyList>

    <path>Chute.ac</path>

    <animation>
        <type>select</type>
        <object-name>chute</object-name>
        <condition>
            <equals>
                <property>sim/multiplay/generic/int[6]</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>scale</type>
        <object-name>chute</object-name>
        <property>sim/multiplay/generic/float[6]</property>
        <x-min>1</x-min>
        <y-min>1</y-min>
        <z-min>1</z-min>
        <x-factor>1</x-factor>
        <y-factor>15</y-factor>
        <z-factor>15</z-factor>
    </animation>

</PropertyList>
